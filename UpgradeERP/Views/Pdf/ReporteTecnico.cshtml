            <div class="row space">
                <div class="column2">
                    <div class="Ruc"><b>R.U.C.</b> - 20454043660</div>
                    <div class="Direccion"><b>Dirección</b> - Rivero 408 Of. 1</div>
                    <div class="Telefono"><b>Telefono</b> - 054-201476</div>
                    <div class="FechaRecepcion"><b>Fecha de Recepcion</b> - @Model.FechaRecepcion.ToString("dd/MM/yyyy")</div>
                </div>
                <div class="column2">
                    <div style="text-align: right;">
                        <b>Nro</b>
                        <div>01-010101</div>
                        <b>Fecha de Entrega</b>
                        <div>@Model.FechaEntrega.ToString("dd/MM/yyyy")</div>
                    </div>
                </div>
            </div>
            <div class="Titulo"></div>

            <div class="Tabla">
                <table class="table-bordered">
                    <thead>
                        <tr>
                            <th class="Row-Title"> </th>
                            <th@* class="Row-Data"*@>REPORTE DE SERVICIO TECNICO</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="Row-Title"><b>FECHA RECEPCION</b></td>
                            <td class="Row-Data">@Model.FechaRecepcion.ToString("dd/MM/yyyy")</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>TIPO</b></td>
                            <td class="Row-Data">@Model.Tipodemantenimiento</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>EQUIPO</b></td>
                            <td class="Row-Data">@Model.Articulo.Producto.Nombre</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>DEJA</b></td>
                            <td class="Row-Data">@Model.ObjetosDejados
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>CLIENTE</b></td>
                            <td class="Row-Data">@Model.Cliente.Persona.Nombre</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>DIRECCION</b></td>
                            <td class="Row-Data">@Model.Cliente.Persona.Direccion</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>TELF/CEL</b></td>
                            <td class="Row-Data">@Model.Cliente.Persona.Telefono</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>ENCARGADO</b></td>
                            <td class="Row-Data">@Model.Encargado.Persona.Nombre</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>FALLAS PREVIAS</b></td>
                            <td class="Row-Data">@Model.Fallas</td>
                        </tr>
                        <tr>
                            <td class="Row-Title"><b>PRUEBAS RESULTADO</b></td>
                            <td class="Row-Data">@Model.Resultado</td>
                        </tr>
                    </tbody>
                </table>

            </div>
            <div class="row">
                <div class="column">

                    <br />
                    <br />
