@model UpgradeERP.Models.SoporteTecnico.ReporteTecnico

@{
    Layout = null;
    ViewData["Title"] = "Reporte Tecnico";
}

<!DOCTYPE html>
<HTML>
<head>
    <meta charset="utf-8">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Darker+Grotesque:wght@300&family=Major+Mono+Display&family=Raleway+Dots&display=swap" rel="stylesheet">
</head>
<body>
    <style>
        :root {
            font-family: 'Darker Grotesque', sans-serif;
            /*font-family: 'Raleway Dots', cursive;*/
        }

        .column {
            /*            background: grey;*/
            float: left;
            width: 50%;
        }

        .column2 {
            /*background: red;*/
            float: left;
            width: 50%;
        }

        /* Clear floats after the columns */
        .row:after {
            content: "";
            display: table;
            clear: both;
        }
        /* Estilar Reporte */
        .Titulo {
            text-align: center;
            font-weight: 600;
        }

        .Sistemas {
            font-family: 'Raleway Dots', cursive;
            text-align: center;
            font-size: 15px;
        }

        .Descripcion {
            text-align: center;
        }

        .container {
            display: grid;
            grid-template-columns: auto auto auto;
            background-color: DodgerBlue;
        }

        .space {
            margin: auto 10px;
        }

        .container > div {
            background-color: #f1f1f1;
            text-align: center;
            margin: 10px;
        }

        .Logo {
            display: block;
            margin: auto;
        }

        .Tabla {
            height: 550px;
            /* border: 1px solid blue;
                            border-radius: 5px;
                            padding: 10px;
                            margin: 10px;*/
        }

        .Autor {
            font-family: 'Raleway Dots', cursive;
            text-align: center;
            font-size: 10px;
        }

        .Row-Title {
            padding: 0;
            color: #052542;
            text-align: right;
        }

        .Row-Data {
            padding-left: 5px;
            border-left: 1px solid #052542;
        }

    </style>

