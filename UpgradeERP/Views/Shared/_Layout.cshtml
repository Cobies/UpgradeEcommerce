
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - UpgradeErp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layout.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>


</head>


<body>
    <div class="d-flex">
        <div id="sidebar-container" class="bg-primary">
            <div class="logo">
                <img src="~/lib/img/logo.png" class="img-fluid"  alt="Responsive image">
            </div>
            <div class="menu h6">

                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#almacen-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Almacen</a>
                    <div class="collapse" id="almacen-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="Productos">Productos</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="ProductosStock">ProductosStock</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="GuiaRemision">Guias Remision</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="LiberacionArticulo">Liberacion Articulo</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="OrdenesDescargaProducto">Descarga Productos</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Almacen" asp-action="OrdenesEntradaProducto">Entrada Productos</a></li>
                        </ul>   
                    </div>
                </li>
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#logistica-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Logistica</a>
                    <div class="collapse" id="logistica-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Logistica" asp-action="FacturasCompra">Facturas de Compra</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Logistica" asp-action="OrdenDeCompra">Orden de Compra</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Logistica" asp-action="SolicitudCompras">Solicitud Compras</a></li>
                        </ul>
                    </div>
                </li>
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#clientes-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Clientes</a>
                    <div class="collapse" id="clientes-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Clientes" asp-action="Clientes">Clientes</a></li>
                        </ul>
                    </div>
                </li>
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#finanzas-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Finanzas</a>
                    <div class="collapse" id="finanzas-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Finanzas" asp-action="Movimientos">Movimientos</a></li>
                        </ul>
                    </div>
                </li>
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#gsc-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo GSC</a>
                    <div class="collapse" id="gsc-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Gsc" asp-action="Garantias">Garantias</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Gsc" asp-action="ServicioTecnico">Servicio Tecnico</a></li>
                        </ul>
                    </div>
                </li>

                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#vendedores-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Vendedores</a>
                    <div class="collapse" id="vendedores-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Vendedores" asp-action="CuotaAnual">Cuota Anual</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Vendedores" asp-action="CuotaPorPuntoVenta">Cuota por punto de venta</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Vendedores" asp-action="CuotasSegmentoDeVenta">Cuota por segmento de venta</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Vendedores" asp-action="CuotaVendedor">Cuota por vendedor</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Vendedores" asp-action="PerfilDeVendedores">Perfil de Vendedores</a></li>

                        </ul>
                    </div>
                </li>
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#ventas-collapse" aria-expanded="false"><i class="bi bi-card-checklist me-2"></i>Modulo de Ordenes de Venta</a>
                    <div class="collapse" id="ventas-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Ventas" asp-action="OrdenesDeVenta">Orden de Venta</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Ventas" asp-action="SeguimientoClientes">Seguimiento de Clientes</a></li>
                        </ul>
                    </div>
                </li>
                <!--Soporte Tecnico Nav-->
                <li class="mb-0 drop">
                    <a href="#" class="dash d-block text-light p-3" data-bs-toggle="collapse" data-bs-target="#gsc-collapse" aria-expanded="false"><i class="bi bi-box-seam me-2"></i>Modulo de Gsc</a>
                    <div class="collapse" id="gsc-collapse">
                        <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Gsc" asp-action="ServicioTecnico">Servicio Tecnico</a></li>
                            <li><a class="dash d-block text-light p-3 rounded" asp-area="" asp-controller="Gsc" asp-action="Garantias">Garantias</a></li>
                        </ul>
                    </div>
                </li>

            </div>
        </div>
        <div class="w-100">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <form class="d-flex position-relative my-2 d-inline-block">
                        <input class="form-control me-0" type="search" placeholder="Buscar" aria-label="Buscar">
                        <button class="btn btn-search border" type="submit"><ion-icon name="search-outline"></ion-icon></button>
                    </form>
                    <div class="collapse navbar-collapse " id="navbarSupportedContent">
                        <ul class="navbar-nav ms-auto mb-2 mb-lg-0">

                            <li class="nav-item dropdown pe-5">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Usuario
                                </a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <li><a class="dropdown-item" href="#">Mi Perfil</a></li>
                                    <li><a class="dropdown-item" href="#">Suscripcion</a></li>
                                    <li><hr class="dropdown-divider"></li>
                                    <li><a class="dropdown-item" href="#">Cerrar Sesion</a></li>
                                </ul>
                            </li>
                        </ul>

                    </div>
                </div>
            </nav>

            <div id="content" class="w-100">
                <section>
                    <div class="container w-100" >
                        <div class="row">
                            <div class="col-lg-9">
                                <h1 class="mb-0">@ViewData["Title"]</h1>
                            </div>

                        </div>
                    </div>
                </section>
                <section>
                    <div class="container">
                                <div class="row">
                            @RenderBody()
                                </div>


                        <div class="row">
                            <section style="height: 100px;"></section>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>


    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - UpgradeMVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script async src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script async src="~/js/site.js" asp-append-version="true"></script>
    <script async src="~/js/input.js"></script>

    @{
        string? tit = @ViewData["Scripts"] as string;
        if (tit != null)
        {
            @await RenderSectionAsync(tit, required: false)
        }
    }
</body>

</html>
